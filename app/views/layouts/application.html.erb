            <li><%= link_to "Courses", '/courses' %></li>
            <li><%= link_to "Cohorts", '/cohorts' %></li>
            <li><%= link_to "Teachers", '/teachers' %></li>
            <li><%= link_to "Students", '/students' %></li>
            <li><%= link_to "Log in", '/jimmies' %></li>
          </ul>
        </nav>
      </div>
    </header>
    <div class="container">
      <div class="row">
        <div class="col">

          <%= yield %>

        </div>
      </div>

    </div>
